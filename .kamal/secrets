SECRETS=$(kamal secrets fetch --adapter 1password --account my.1password.eu --from Infrastructure/stanko.io "Container Registry Password" "production/Master Key" "Sentry DSN")

KAMAL_REGISTRY_PASSWORD=$(kamal secrets extract "Container Registry Password" $SECRETS)
RAILS_MASTER_KEY=$(kamal secrets extract "production/Master Key" $SECRETS)
SENTRY_DSN=$(kamal secrets extract "Sentry DSN" $SECRETS)
