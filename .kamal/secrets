SECRETS=$(kamal secrets fetch --adapter 1password --account my.1password.eu --from Infrastructure/stanko.io "Docker Hub Token" "production/Master Key" "AppSignal Push Key")

KAMAL_REGISTRY_PASSWORD=$(kamal secrets extract "Docker Hub Token" $SECRETS)
RAILS_MASTER_KEY=$(kamal secrets extract "production/Master Key" $SECRETS)
APPSIGNAL_PUSH_API_KEY=$(kamal secrets extract "AppSignal Push Key" $SECRETS)
